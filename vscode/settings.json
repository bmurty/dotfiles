{
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "clock.alignment": "Right",
  "clock.format": "h:MMtt",
  "clock.iconName": "none",
  "css.validate": false,
  "debug.console.fontSize": 20,
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.extension.enableComposeLanguageServer": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.fontFamily": "CommitMono",
  "editor.fontLigatures": true,
  "editor.fontSize": 24,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 4000,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "comments": "#666",
    "textMateRules": [
      {
        "scope": [
          "comment"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.eol": "\n",
  "geminicodeassist.enableTelemetry": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.inputValidation": false,
  "git.inputValidationLength": 150,
  "git.inputValidationSubjectLength": null,
  "git.openRepositoryInParentFolders": "never",
  "git.untrackedChanges": "separate",
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "indentRainbow.colors": [
    "rgba(255,255,64,0.04)",
    "rgba(127,255,127,0.04)",
    "rgba(255,127,255,0.04)",
    "rgba(79,236,236,0.04)"
  ],
  "indentRainbow.errorColor": "rgba(128,32,32,0.5)",
  "indentRainbow.tabmixColor": "rgba(128,32,96,0.5)",
  "javascript.validate.enable": false,
  "json.schemas": [
    {
      "fileMatch": [
        "deno.json",
        "deno.jsonc"
      ],
      "url": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json"
    },
    {
      "fileMatch": [
        "package.json"
      ],
      "url": "https://www.schemastore.org/package"
    }
  ],
  "markdown.preview.fontFamily": "CommitMono",
  "markdown.preview.fontSize": 22,
  "redhat.telemetry.enabled": false,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "CommitMono",
  "terminal.integrated.fontSize": 20,
  "terminal.integrated.scrollback": 2500,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
  "terminal.integrated.tabStopWidth": 4,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "window.commandCenter": true,
  "window.confirmSaveUntitledWorkspace": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "visible",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#23c5b0",
    "activityBar.background": "#000",
    "activityBar.foreground": "#23c5b0",
    "activityBar.inactiveForeground": "#666",
    "activityBarBadge.background": "#23c5b0",
    "activityBarBadge.foreground": "#000",
    "contrastBorder": "#222",
    "editor.background": "#000",
    "editorLineNumber.activeForeground": "#888888",
    "editorLineNumber.foreground": "#444444",
    "sideBar.background": "#000",
    "sideBar.foreground": "#FFF",
    "sideBarSectionHeader.background": "#000",
    "sideBarSectionHeader.foreground": "#23c5b0",
    "statusBar.background": "#000",
    "statusBar.debuggingBackground": "#000",
    "statusBar.noFolderBackground": "#000",
    "tab.activeBackground": "#000",
    "tab.activeBorderTop": "#23c5b0",
    "tab.inactiveBackground": "#111",
    "window.activeBorder": "#000",
    "window.inactiveBorder": "#000"
  },
  "workbench.colorTheme": "Copilot Theme - Higher Contrast",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.list.openMode": "doubleClick",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "https://raw.githubusercontent.com/compose-spec/compose-go/master/schema/compose-spec.json": [
      "docker-compose.yml"
    ],
    "https://www.schemastore.org/github-workflow": [
      ".github/*.yml",
      ".github/**/*.yml"
    ]
  }
}