{
    "window.commandCenter": true,
    "workbench.colorTheme": "Cursor Dark High Contrast",
    "editor.fontFamily": "'CommitMono', 'monospace', monospace",
    "editor.fontSize": 22,
    "editor.minimap.sectionHeaderFontSize": 14,
    "debug.console.fontSize": 22,
    "scm.inputFontSize": 14,
    "markdown.preview.fontSize": 22,
    "cursor.composer.textSizeScale": 1.15,
    "cursor.composer.usageSummaryDisplay": "always",
    "editor.quickSuggestions": {
        "comments": false,
        "other": true,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 4000,
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "comments": "#666",
        "textMateRules": [
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.eol": "\n",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.untrackedChanges": "separate",
    "indentRainbow.colors": [
        "rgba(255,255,64,0.04)",
        "rgba(127,255,127,0.04)",
        "rgba(255,127,255,0.04)",
        "rgba(79,236,236,0.04)"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#23c5b0",
        "activityBar.background": "#000",
        "activityBar.foreground": "#23c5b0",
        "activityBar.inactiveForeground": "#666",
        "activityBarBadge.background": "#23c5b0",
        "activityBarBadge.foreground": "#000",
        "contrastBorder": "#222",
        "editor.background": "#000",
        "editorLineNumber.activeForeground": "#888888",
        "editorLineNumber.foreground": "#444444",
        "sideBar.background": "#000",
        "sideBar.foreground": "#FFF",
        "sideBarSectionHeader.background": "#000",
        "sideBarSectionHeader.foreground": "#23c5b0",
        "statusBar.background": "#000",
        "statusBar.debuggingBackground": "#000",
        "statusBar.noFolderBackground": "#000",
        "tab.activeBackground": "#000",
        "tab.activeBorderTop": "#23c5b0",
        "tab.inactiveBackground": "#111",
        "window.activeBorder": "#000",
        "window.inactiveBorder": "#000"
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.list.openMode": "doubleClick",
    "workbench.startupEditor": "none",
    "git.openRepositoryInParentFolders": "never",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.fontSize": 20,
    "terminal.integrated.fontFamily": "CommitMono Regular",
    "terminal.integrated.scrollback": 2500,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
    "terminal.integrated.tabStopWidth": 4,
    "git.inputValidation": false,
    "git.inputValidationSubjectLength": null,
    "git.inputValidationLength": 150,
    "editor.stickyScroll.enabled": true,
    "workbench.editor.empty.hint": "hidden",
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "workbench.sideBar.location": "left",
    "window.confirmSaveUntitledWorkspace": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "window.customTitleBarVisibility": "auto",
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "docker-desktop"
    ],
    "security.promptForLocalFileProtocolHandling": false,
    "redhat.telemetry.enabled": false,
    "accessibility.signals.lineHasInlineSuggestion": {
        "sound": "off"
    },
    "window.menuBarVisibility": "visible",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.extension.enableComposeLanguageServer": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "geminicodeassist.displayInlineContextHint": false,
    "geminicodeassist.inlineSuggestions.suggestionSpeed": "Slow",
    "geminicodeassist.inlineSuggestions.enableAuto": false,
    "editor.screenReaderAnnounceInlineSuggestion": false
}
